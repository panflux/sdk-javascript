{
    "name": "@panflux/sdk",
    "version": "0.1.0",
    "description": "Software Development Kit for Panflux API consumers",
    "repository": "github:panflux/sdk-javascript",
    "author": "Niels Keurentjes <niels.keurentjes@omines.com>",
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --fix src",
        "test": "eslint src && jest --detectOpenHandles --forceExit",
        "test-insecure": "eslint src && NODE_TLS_REJECT_UNAUTHORIZED=0 jest --detectOpenHandles --forceExit"
    },
    "dependencies": {
        "apollo-link-context": "^1.0.9",
        "apollo-link-error": "^1.1.1",
        "apollo-link-http": "^1.5.5",
        "apollo-link-ws": "^1.0.9",
        "cross-fetch": "^2.2.2",
        "eventemitter3": "^3.1.0",
        "graphql": "^14.0.2",
        "graphql-tag": "^2.10.0",
        "subscriptions-transport-ws": "^0.9.15"
    },
    "devDependencies": {
        "dotenv": "6.1.0",
        "eslint": "5.8.0",
        "eslint-config-google": "0.11.0",
        "eslint-plugin-import": "2.14.0",
        "jest": "23.6.0"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.js"
        ]
    },
    "files": [
        "src/*"
    ]
}
